YH_SECTOR_DELAY_MIN_MS=100
YH_SEARCH_DELAY_MAX_MS=1200
YH_SECTOR_DELAY_MAX_MS=1500
YH_SEARCH_RETRY_MAX=3
YH_SECTOR_RETRY_MAX=3
